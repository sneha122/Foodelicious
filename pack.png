<script id="addressBlockerTemplate" type="text/template">
    <div class="ui addressblocker small modal <% if (persistent) { %> persistent-modal <% } %>">
        <% if (!persistent) { %>
            <i class="close icon"></i>
        <% } else if (persistent == 'no-add') { %>
            <i class="close icon back-to-res"></i>
        <% } else if (persistent == 'first-item') { %>
            <i class="close icon remove-first-item"></i>
        <% } %>
        <div class="header">
            Select or add a delivery address
        </div>
        <div class="content"  >
            <div class="address-list">
                <% if (addresses.length) { %>
                <ul>
                    <% _.each(addresses, function(address) {
                    if(address.status) { 
                        if (!isMobile) {
                            address.alias = RemoveHTMLTagsImproved(address.alias);
                        }
                        %>
                    <li class="save-address" data-address_id="<%= address.id %>">
                        <div class="ui radio checkbox block-label">
                            <input type="radio" id="<%= address.id %>" value="<%= address.id %>" name="address" <% if(selected_address == address.id){ %>checked<% } %> data-minorder="<%= address.min_order %>" data-below-minorder="<%= address.accept_below_min_order %>" />
                            <label for="<%= address.id %>">
                                <div class="bold"><%= address.alias %></div>
                                <div class="full-address stdlabel"><%= address.address+", "+address.delivery_subzone_name %></div>
                            </label>
                        </div>
                    </li>
                    <% } %>
                    <% }); %>
                </ul>
                <% } %>
                                <div class="ui button save-address add-address">
                    Add New Address
                </div>
                                <% if (invalid_addresses.length) { %>
                <div class="no_delivery_container mtop0">
                    <div class="category-heading">Does not Deliver to</div>
                    <ul>
                        <% _.each(invalid_addresses, function(address) { %>
                        <li class="save-address no_delivery_address">
                            <div class="ui radio checkbox">
                                <input type="radio" disabled readonly />
                                <label>
                                    <div class="bold"><%= address.alias %></div>
                                    <div class="full-address stdlabel"><%= address.address+", "+address.delivery_subzone_name %></div>
                                </label>
                            </div>
                        </li>
                        <% }); %>
                    </ul>
                </div>
                <% } %>
                <div class="mtop0 brstd address-errors error-message-highlight-small hidden"></div>
            </div>
        </div>
        <div class="actions">
            <div class="ui positive button <% if(!selected_address){ %>disabled<% } %> save-valid-address">Continue</div>
        </div>
    </div>
</div>
</script>

<script id="addAddressTemplate" type="text/template">
<div class="ui addaddress small modal <% if (noexit !== undefined) { %> persistent-modal <% } %>">
    <% if (noexit == 'noexit') { %>
        <i class="close icon back-to-res"></i>
    <% } else if ( noexit == 'first') { %>
        <i class="close icon"></i>
    <% } else { %>
        <i class="close icon back-address"></i>
    <% } %>
    <div class="header">
        Add Delivery Address
   </div>
   <div class="content add_address_modal ui form">

       <div class="address-field">
           <div class="row">
                <div class="col-s-13 col-m-14">
                    <div class="ui search subzones">
                        <div class="ui icon input" style="width: 100%;">
                            <input class="prompt" type="text" placeholder="Delivery Area">
                            <i class="green checkmark icon" style="display: none;"></i>
                            <i class="yellow warning sign icon" style="display: none;"></i>
                        </div>
                        <div class="results" style="width: 100%;"></div>
                    </div>
                </div>
                <div class="col-s-1">
                    <div class="ui button get-location w100" data-icon="8"></div>
                </div>
           </div>
           <div class="row">
               <div class="col-s-16 mt10">
                    <div class="ui warning message get-location-errors location-errors hidden"></div>
                </div>
           </div>
        </div>

        <div class="row">
            <div class="col-s-16 address-field">
                <input class="locality" type="text" placeholder="Complete Address e.g. house number, street name, landmark" />
            </div>
        </div>
        

        <div class="row">
            <div class="col-s-16 address-field">
                <textarea class="add-inst" rows="2" placeholder="Delivery Instructions e.g. Opposite Gold Souk Mall"></textarea>
            </div>
        </div>
        
        <div class="row">
            <div class="col-s-16 address-field">
                <div class="nickname-options">
                    <a class="ui large basic label">Home</a>
                    <a class="ui large basic label">Work</a>
                    <a class="ui large basic label">Other</a>
                    <div class="other-option pt10 hidden">
                        <input class="alias" type="text" placeholder="Nickname e.g. Home, Work etc" />
                    </div>
                </div>
            </div>
        </div>

        <div class="ui error message new-address-errors content hidden">Please enter missing data.</div>
    </div>
    <div class="actions">
        <% if(!no_address) { %>
            <div class="ui button left back-address">Back</div>
        <% } %>  
        <div class="ui green button save-new-address">Save</div>
    </div>
</div>
</script>

