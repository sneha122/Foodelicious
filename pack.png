<script id="vendorAddAddressTemplate" type="text/template">
<div class="ui vendor-addaddress small modal">
    <i class="close icon"></i>
    <div class="header">
        Add Delivery Address
    </div>
    <div class="content">
        <div class="addr-inst"><% if(vendor == 'dominos') { %> Domino's <% } else if(vendor == 'pizza_hut') { %> Pizza Hut <% } else if(vendor == 'mcd') { %> McDonald's <% } %>needs your address to find the outlet closest to you.</div>
        
        <% if (vendor == 'mcd') { %>
        <div class="address-field">
            <div class="ui search search-vendor-locality">
                <input class="prompt" type="text" placeholder="Please select a locality">
                <div class="results"></div>
            </div>
        </div>
        <% } else { %>
        <div class="address-field">
            <select class="ui search selection dropdown vendor-locality">
                <option value="" disabled selected>Please select a locality</option>
                <%= renderVendorLocalityOptions() %>
            </select>
        </div>
        <% } %>

        <div class="address-field">
            <span class="vendor-sub-locality-span">
                <select class="ui search selection dropdown vendor-sub-locality"></select>
            </span>
        </div>

        <% if (vendor !== 'pizza_hut') { %>
          <div class="address-field">
              <input class="flat_no" type="text" placeholder="Flat no. e.g. locality, house no. <% if(vendor == 'dominos' || vendor == 'mcd') { %>(Max 10 Characters)<% } %>" />
          </div>
        <% } %>
        <% if (vendor !== 'mcd') { %>
        <div class="address-field">
            <input class="street_add" type="text" placeholder="<% if (vendor == 'dominos') {
                                                               %>Street Address e.g. Street name etc<%
                                                               } else if (vendor == 'pizza_hut') {
                                                               %>Complete Address e.g. house number, street name (Max 28 Characters)<%
                                                               } %>"/>
        </div>
        <% } %>
        <div class="ui warning message vendor-address-errors content hidden">Please fill required fields. (Special characters not allowed)</div>
        <div class="ui error message vendor-store-errors content hidden"></div>
    </div>
    <div class="actions">
        <div class="ui button green save-new-address">Proceed</div>
    </div>
